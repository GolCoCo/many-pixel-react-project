{
    "name": "manypixels-platform",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "vite",
        "build": "vite build",
        "build-view": "vite preview",
        "serve": "pm2-runtime start ecosystem.config.js --env production",
        "maintenance": "pm2-runtime start ecosystem.config.js --env maintenance",
        "dev": "npm run bun-start-dev",
        "bun-start-dev": "bunx --bun vite",
        "bun-build": "bunx --bun vite build",
        "bun-start": "bunx --bun vite preview --port 4000",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "env:encrypt": "npx dotenv-encode .env -o .env.encrypted",
        "env:decrypt": "npx dotenv-encode .env.encrypted -o .env -d",
        "prebuild": "npm run copy-assets",
        "copy-assets": "cpx 'node_modules/pdfjs-dist/build/pdf.worker.min.js' 'public/' && cpx 'node_modules/pdfjs-dist/iccs/**/*' public/iccs && cpx 'node_modules/pdfjs-dist/cmaps/**/*' public/cmaps && cpx 'node_modules/pdfjs-dist/wasm/**/*' public/wasm"
    },
    "dependencies": {
        "@ant-design/colors": "^7.1.0",
        "@ant-design/icons": "^4.8.3",
        "@apollo/client": "^3.12.4",
        "@apollo/react-hooks": "^4.0.0",
        "@rollup/plugin-commonjs": "^26.0.1",
        "@stripe/react-stripe-js": "^3.3.0",
        "@stripe/stripe-js": "^5.9.2",
        "antd": "4.24.16",
        "apollo-upload-client": "^10.0.1",
        "axios": "^1.7.7",
        "babel-plugin-styled-components": "^2.1.4",
        "d3": "^7.9.0",
        "dompurify": "^3.2.5",
        "dotenv-encode": "^1.0.5",
        "draft-js": "^0.11.7",
        "file-saver": "^2.0.5",
        "graphql": "^16.10.0",
        "graphql-ws": "^5.16.2",
        "history": "^4.10.1",
        "jszip": "^3.10.1",
        "linkify-it": "^3.0.3",
        "lodash": "^4.17.21",
        "merge-images": "^2.0.0",
        "moment": "^2.30.1",
        "mutationobserver-shim": "^0.3.7",
        "numeral": "^2.0.6",
        "pdfjs-dist": "^5.3.31",
        "prop-types": "^15.8.1",
        "qs": "^6.13.0",
        "query-string": "^6.14.1",
        "quill-mention": "^6.0.2",
        "react": "^16.14.0",
        "react-beautiful-dnd": "^13.1.1",
        "react-dom": "^16.14.0",
        "react-dropzone": "^11.7.1",
        "react-hook-form": "^7.53.0",
        "react-process-string": "^1.2.0",
        "react-quill-new": "^3.4.6",
        "react-responsive-carousel": "^3.2.23",
        "react-router": "^5.1.2",
        "react-router-dom": "^5.1.2",
        "styled-components": "^6.1.17",
        "tlds": "^1.254.0",
        "uuid": "^9.0.1",
        "vanilla-sharing": "^6.1.2"
    },
    "devDependencies": {
        "@types/draft-js": "^0.11.18",
        "@types/linkify-it": "^5.0.0",
        "@types/react": "^16.14.0",
        "@types/react-beautiful-dnd": "^13.1.8",
        "@types/react-router-dom": "^5.3.3",
        "@types/styled-components": "^5.1.34",
        "@vitejs/plugin-react": "^4.3.1",
        "babel-eslint": "^10.1.0",
        "cpx2": "^4.2.3",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-prettier": "^6.15.0",
        "eslint-plugin-flowtype": "^4.7.0",
        "eslint-plugin-import": "^2.30.0",
        "eslint-plugin-jsx-a11y": "^6.10.0",
        "eslint-plugin-prettier": "^3.4.1",
        "eslint-plugin-react": "^7.35.2",
        "eslint-plugin-react-hooks": "^1.7.0",
        "less": "^4.2.0",
        "pm2": "^5.4.2",
        "prettier": "^2.1.2",
        "sass": "^1.78.0",
        "vite": "^5.4.3",
        "vite-plugin-compression2": "^1.3.3"
    },
    "resolutions": {
        "react-is": "^16.13.1"
    },
    "volta": {
        "node": "22.15.0"
    }
}
